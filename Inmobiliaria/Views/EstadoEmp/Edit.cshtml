@model InmobiliariaViewModels.Empleados.EstadoEmpViewModels

@{
    ViewBag.Title = "Edit";
}

<div class="row">
    <div class="col s12 m12 l12">
        <h2 class="blue-text text-darken-1">Editar Estado</h2>
        @using (Html.BeginForm("Edit", "EstadoEmp", FormMethod.Post))
        {
            <div class="col s12 m4 l5">
                <label for="Nombre">Nombre</label>
                <input type="text" id="nestado" name="nestado" class="validate" value="@Model.nestado">
            </div>
            <div class="col s12 m4 l5">
                <button type="submit" class="btn light-blue">Actualizar</button>
            </div>
        }
    </div>
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/Scripts/js/materialscript.js")
}