@model List<InmobiliariaViewModels.Cuotas.CuotasPorFinanciamientoViewModels>
@{
    ViewBag.Title = "Details";
}


<div class="row">
    <a class="btn blue" href="@Url.Action("ShowFinanciamientoReport", "Cuota")">Ver Reporte</a>
    <a class="btn gren" href="@Url.Action("DownloadFinanciamientoReport", "Cuota")">Descargar Reporte</a>
</div>

<div class="row">
    <div class="col s12 m12 l12">

        <div class="page-header">
            <h4 id="tables" class="text-center">Cuotas Por Financiamiento</h4>
        </div>

        <table class="striped responsive-table">
                    <thead>
                        <tr>
                            <th scope="col">Precio Total</th>
                            <th scope="col">Meses</th>
                            <th scope="col">Enganche</th>
                            <th scope="col">Precio Venta</th>
                            <th scope="col">Intereses</th>
                            <th scope="col">Precio Mas Interes</th>
                            <th scope="col">Cuotas</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td scope="col">@item.PrecioBruto</td>
                                <td scope="col">@item.Meses</td>
                                <td scope="col">@item.Enganche</td>
                                <td scope="col">@item.PrecioNeto</td>
                                <td scope="col">@item.InteresTotal</td>
                                <td scope="col">@item.PrecioTotal</td>
                                <td scope="col">@item.Cuotas</td>
                            </tr>
                        }
                    </tbody>
                </table>

    </div>
</div>

