@model List<InmobiliariaViewModels.Cuotas.CuotasPorMesViewModels>

@{
    ViewBag.Title = "Details";
}

<div class="row">
    <div class="col s12 m12 l12">

        <div class="page-header">
            <h4 id="tables" class="text-center">Cuotas Mensuales</h4>
        </div>
            <table class="striped responsive-table">
                    <thead>
                        <tr>
                            <th scope="col">#</th>
                            <th scope="col">Cuota Sin Interes</th>
                            <th scope="col">Interes</th>
                            <th scope="col">Cuota Total</th>
                            <th scope="col">Saldo Sin Interes</th>
                            <th scope="col">Interes Total</th>
                            <th scope="col">Saldo con Interes</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td scope="col">1</td>
                                <td scope="col">@item.CuotaSinInteres</td>
                                <td scope="col">@item.InteresPorMes</td>
                                <td scope="col">@item.CuotaConInteres</td>
                                <td scope="col">@item.SaldoPrecioSinInteres</td>
                                <td scope="col">@item.SaldoInteresTotal</td>
                                <td scope="col">@item.SaldoPrecioConInteres</td>
                            
                            </tr>
                        }
                    </tbody>
            </table>
    </div>
</div>