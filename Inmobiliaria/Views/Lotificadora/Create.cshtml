@model InmobiliariaViewModels.Lotificadora.LotificadoraViewModels
@{
    ViewBag.Title = "Ingresar Lotificadora";
}
<div class="row">
    <div class="col s12 m12 l12">
        <h2 class="blue-text text-darken-1">Ingresar Lotificadora</h2>
        @using (Html.BeginForm("Create", "Lotificadora", FormMethod.Post))
        {
            <div class="input-field col s12 m4 l5">
                <label for="Nombre">Nombre</label>
                <input type="text" id="nombre" name="nombre" class="validate">
            </div>
            <div class="input-field col s12 m4 l5">
                <label for="Direccion">Direccion</label>
                <input type="text" id="direccion" name="direccion" class="validate">
            </div>
            <div class="input-field col s12 m4 l5">
                <label for="Telefono">Telefono</label>
                <input type="text" id="telefono" name="telefono" class="validate">
            </div>
            <div class="input-field col s12 m4 l5">
                <label for="Tasa de Interes">Tasa de Interes</label>
                <input type="text" id="tasa_interes" name="tasa_interes" class="validate">
            </div>
            <div class="input-field col s12 m4 l5">
                <label for="Tasa de Mora">Tasa de Mora</label>
                <input type="text" id="tasa_mora" name="tasa_mora" class="validate">
            </div>

            <div class="col s12 m4 l5">
                <button type="submit" class="btn light-blue"><i class="fas fa-save"></i>Guardar</button>
            </div>
        }
    </div>
</div>
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/Scripts/js/materialscript.js")
}