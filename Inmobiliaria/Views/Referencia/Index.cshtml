@model List<InmobiliariaViewModels.Clientes.ClienteReferenciaViewModels>
@{
    ViewBag.Title = "Index";
}


<div class="row">
    <div class="col s12 m12 l12">
        <h4 class="text-center blue-text text-darken-1">Tabla Referencias</h4>

        <table class="striped responsive-table">
            <thead >
                <tr>
                    <th scope="col">Nombre</th>
                    <th scope="col">Apellido</th>
                    <th scope="col">Telefono</th>
                    <th scope="col">Direccion</th>
                    <th scope="col">Cliente</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td scope="col">@item.Nombre</td>
                        <td scope="col">@item.Apellido</td>
                        <td scope="col">@item.Telefono</td>
                        <td scope="col">@item.Direccion</td>
                        <td scope="col">@item.ClienteId</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>



