@model InmobiliariaViewModels.Lotes.ManzanaLoteViewModels
@{
    ViewBag.Title = "Edit";
}


<div class="row">
    <div class="col s12 m12 l12">
        <h2 class="blue-text text-darken-1">Editar Manzana</h2>
        @using (Html.BeginForm("Edit", "ManzanaLote", FormMethod.Post))
        {
            <div class="input-field col s12 m4 l5">
                <label for="manzana">Manzana</label>
                <input type="text" name="manzana" id="manzana" class="validate" value="@Model.manzana"/>
            </div>
            <div class="col s12 m4 l5">
                <button type="submit" class="btn light-blue">Actualizar</button>
            </div>
        }
    </div>
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/Scripts/js/materialscript.js")
}